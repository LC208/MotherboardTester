<Window x:Class="MotherboardTester.MultimeterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Мультиметр" Height="88" Width="140">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="15"></RowDefinition>
            <RowDefinition Height="15"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="13"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBlock Text="U: " Grid.Column="0" Grid.Row="0"/>
        <TextBlock x:Name="vValue" Text="0.0" Grid.Column="1" Grid.Row="0"/>
        <TextBlock Text="V" Grid.Column="2" Grid.Row="0"/>
        <TextBlock Text="R: " Grid.Column="0" Grid.Row="1"/>
        <TextBlock x:Name="rValue" Text="0.0" Grid.Column="1" Grid.Row="1"/>
        <TextBlock Text="kOM" Grid.Column="2" Grid.Row="1"/>
    </Grid>
</Window>
